{
  "reactions": [
    {
      "equation": "E + S -> ES",
      "rate_constant": 1.0
    },
    {
      "equation": "ES -> E + S",
      "rate_constant": 0.5
    },
    {
      "equation": "ES -> E + P",
      "rate_constant": 0.2
    }
  ],
  "initial_conditions": {
    "E": 1.0,
    "S": 10.0,
    "ES": 0.0,
    "P": 0.0
  },
  "simulation": {
    "time_span": [0, 50],
    "method": "LSODA"
  }
}
